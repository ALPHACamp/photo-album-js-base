<%= form_for @photo, :url => photo_path(@photo), :method => :patch do |f| %>
  <%= f.label :title, "Title" %>
  <%= f.text_field :title %>

  <%= f.label :date, "Date" %>
  <%= f.text_field :date %>

  <%= f.label :description, "Description" %>
  <%= f.text_area :description %>

  <%= f.label :file_location, "File Location" %>
  <%= f.text_field :file_location %>

  <%= f.submit "Update" %>
<% end %>
